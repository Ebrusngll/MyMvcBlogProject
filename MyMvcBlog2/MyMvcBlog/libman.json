{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.0.0",
      "destination": "wwwroot/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "js/bootstrap.js",
        "css/bootstrap.css",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "twitter-bootstrap@4.6.0",
      "destination": "wwwroot/js/",
      "files": [
        "js/bootstrap.min.js",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.js"
      ]
    }
  ]
}